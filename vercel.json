{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.6.0"
    }
  },
  "routes": [
	{ "src": "/index.php",  "dest": "/api/index.php" },
	{ "src": "/hello.php",  "dest": "/api/hello.php" },
	{ "src": "/hello",  "dest": "/api/api.php" },
	{ "src": "/welcome",  "dest": "/api/welcome.php" },
	{ "src": "/(.*)",  "dest": "/public/$1" }
  ]
}